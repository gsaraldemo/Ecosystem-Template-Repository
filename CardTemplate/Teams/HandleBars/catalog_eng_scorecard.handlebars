{
  "type": "AdaptiveCard",
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.2",
  "body": [
    {{#each message}}
    {
      "type": "Container",
      "items": [
		{
		  "type": "FactSet",
		  "facts": [
			{
			  "title": "Service",
			  "value": "{{this.data.service.name}}"
			},
			{
			  "title": "Maintainer",
			  "value": "{{this.data.service.maintainer.name}}"
			}
		  ]
		}
      ]
    },
    {{/each}}
    {
      "type": "Container",
      "items": [
      	{
		{
                    "type": "TextBlock",
                    "text": "Powered by teams integration",
                    "wrap": true
                }
	}
      ]
    }
  ]
}
